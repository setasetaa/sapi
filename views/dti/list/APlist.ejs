                  <div class="custom-control custom-checkbox custom-control-inline">
                    <input type="checkbox" class="custom-control-input" id="dtiStatus3" name="dtiStatus" value="C">
                    <label class="custom-control-label" for="dtiStatus3">승인</label>
                  </div>
                  <div class="custom-control custom-checkbox custom-control-inline">
                    <input type="checkbox" class="custom-control-input" id="dtiStatus4" name="dtiStatus" value="R">
                    <label class="custom-control-label" for="dtiStatus4">수신거부</label>
                  </div>
                  <div class="custom-control custom-checkbox custom-control-inline">
                    <input type="checkbox" class="custom-control-input" id="dtiStatus5" name="dtiStatus" value="O">
                    <label class="custom-control-label" for="dtiStatus5">발행취소</label>
                  </div>
                  <div class="custom-control custom-checkbox custom-control-inline">
                    <input type="checkbox" class="custom-control-input" id="dtiStatus6" name="dtiStatus" value="V">
                    <label class="custom-control-label" for="dtiStatus6">역발행요청</label>
