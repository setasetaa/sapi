                      <input type="checkbox" class="custom-control-input" id="dtiStatus8" name="dtiStatus" value="T">
                      <label class="custom-control-label" for="dtiStatus7">역발행요청거부</label>
                    </div>
                  <div class="d-inline float-right">
                    <button type="button" id="SBData"  class="btn btn-outline-primary">SBData</button>
                    <button type="button" id="exelDown" class="btn btn-outline-success">ExelDown</button>
                    <button type="button" id="listSearch" onclick="search('AP')" class="btn btn-outline-primary-sm">Search</button>
