  <!-- SBData Modal-->
  <div class="modal" id="SBModal">
      <div class="modal-content">

        <!-- Modal Header -->
        <div class="modal-header">
          <h4 class="modal-title">SB APList</h4>
          <button type="button" id="SBClose" class="close" data-dismiss="modal">&times;</button>
        </div>

        <!-- Modal body -->
        <div class="modal-body">
          <div class="table-responsive">
            <div class="form-group">
              <label>From Date
              <input type="date" id="SBfrom" name="SBfrom" class="form-control form-control-sm" >
            </label>
              <label>End Date
              <input type="date" id="SBend" name="SBend" class="form-control form-control-sm" >
            </label>
              <button type="button" id="SBSearch" onclick="sbSearch('AP')" class="btn btn-outline-primary-sm">Search</button>
              <button type="button" id="upload" onclick="upload('AP')" class="btn btn-outline-success">Upload</button>
            </div>
            <table class="display select" id="t2" cellspacing="0" width="100%"></table>
          </div>
        </div>
      </div>
  </div>

